{
    "WhileTb": {
      "prefix": "While em Tabela ADVPL",
      "body": [
        "\tWhile ${tabela}->(! Eof())",
        "\t\t${processamento}",
        "\t\t${tabela}->(DbSkip())",
        "\tEndDo",
        "\t${cursor}"
      ],
      "description": "Estrutura de loop WHILE para percorrer uma tabela em ADVPL"
    },
    "For": {
      "prefix": "For ADVPL",
      "body": [
        "\tFor ${var} := 1 to Len(${array})",
        "\t\t${processamento}",
        "\tNext ${var}",
        "\t${cursor}"
      ],
      "description": "Estrutura de loop FOR para percorrer um array em ADVPL"
    },
    "Case": {
      "prefix": "Case ADVPL",
      "body": [
        "\tDo Case",
        "\t\tCase ${condicao1}",
        "\t\t\t${acao1}",
        "\t\tCase ${condicao2}",
        "\t\t\t${acao2}",
        "\t\tCase ${condicao3}",
        "\t\t\t${acao3}",
        "\t\tCase ${condicao4}",
        "\t\t\t${acao4}",
        "\t\tOtherWise",
        "\t\t\t${acaoDefault}",
        "\tEndCase",
        "\t${cursor}"
      ],
      "description": "Estrutura de decisão CASE em ADVPL"
    },
    "If": {
      "prefix": "If Complexo ADVPL",
      "body": [
        "\tIf ${condicao1}",
        "\t\t${acao1}",
        "\tElseIf ${condicao2}",
        "\t\t${acao2}",
        "\tElse",
        "\t\t${acaoDefault}",
        "\tEndIf",
        "\t${cursor}"
      ],
      "description": "Estrutura de decisão IF Complexo em ADVPL"
    },
    "BeginSql": {
      "prefix": "BeginSql",
      "body": [
        "\tBeginSql Alias ${tabela}",
        "\t\tSELECT",
        "\t\t\t${campos}",
        "\t\tFROM ${alias}",
        "\t\tWHERE D_E_L_E_T_ = ' ' ${demaisCondicoes}",
        "\t\tORDER BY ${orderBy}",
        "\tEndSql",
        "\t${cursor}"
      ],
      "description": "Estrutura para execução de comandos SQL em ADVPL"
    },
    "DOC": {
    "prefix": "Doc ADVPL",
    "body": [
        "/*/{Protheus.doc} ${nomeFuncao}",
        "(${descricao})",
        "@author ${autor}",
        "@since ${data}",
        "@version ${versao}",
        "@param ${parametros}, ${descricaoParametros}",
        "@return ${retorno}, ${descricaoRetorno}",
        "@example",
        "${exemplos}",
        "@see ${referencias}",
        "/*/"
    ],
    "description": "Modelo de documentação de código ADVPL"
    },
    "Function": {
    "prefix": "Function ADVPL",
    "body": [
        "User Function ${nomeFuncao}(${parametros})",
        "\tLocal ${variaveisLocais}",
        "\t${processamento}",
        "\tReturn ${retorno}",
        "Return"
    ],
    "description": "Modelo de função ADVPL"
    },
    "UserFunction": {
    "prefix": "UserFunction ADVPL",
    "body": [
        "User Function ${nomeFuncao}(${parametros})",
        "\tLocal ${variaveisLocais}",
        "\t${processamento}",
        "\tReturn ${retorno}",
        "Return"
    ],
    "description": "Modelo de função ADVPL"
    },
    "StaticFunction": {
    "prefix": "StaticFunction ADVPL",
    "body": [
        "Static Function ${nomeFuncao}(${parametros})",
        "\tLocal ${variaveisLocais}",
        "\t${processamento}",
        "\tReturn ${retorno}",
        "Return"
    ],
    "description": "Modelo de função estática ADVPL"
    },
    "BeginSqlIf": {
        "prefix": "BeginSqlIf",
        "body": [
            "\tBeginSql Alias ${tabela}",
            "\t\tSELECT",
            "\t\t\t${campos}",
            "\t\tFROM ${alias}",
            "\t\tWHERE D_E_L_E_T_ = ' ' ${demaisCondicoes}",
            "\t\tORDER BY ${orderBy}",
            "\tEndSql",
            "\tIf ${condicao}",
            "\t\t${acao}",
            "\tEndIf",
            "\t${cursor}"
        ],
        "description": "Cria a estrutura completa do BeginSql com If no resultado"
    },
    "BeginSqlWhile": {
        "prefix": "BeginSqlWhile",
        "body": [
